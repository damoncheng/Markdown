#include <iostream>
using namespace std;
#include "include/function.h"

const static char * func_name = "function";
extern const char * tag;

namespace damoncheng {

    int custom_max(int x, int y){
        return (x>y)?x:y;
    }

    int custom_min(int x, int y){
        return (x>y)?y:x;
    }

    void print_tag(){
        const char * tag = "hello print_tag";
        cout << func_name << " say:" << ::tag << endl;
    }


}


